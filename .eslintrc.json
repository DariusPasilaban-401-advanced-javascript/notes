// 20201013131555
// https://raw.githubusercontent.com/codefellows/seattle-javascript-401d38/master/reference/submission-instructions/code-challenges/example/data-structures-and-algorithms/.eslintrc.json

{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "globals": {
    "err": true,
    "req": true,
    "res": true,
    "next": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": "off",
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}